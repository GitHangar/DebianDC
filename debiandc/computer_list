cd /usr/local/debiandc
source common-value

samba-tool computer list > /tmp/computerlist
sed -i '$! {}; s/.$//' /tmp/computerlist
cat /tmp/computerlist | zenity --text-info --title="Active Directory Computer List" --width $WIDTH --height $HEIGHT

if [ "$?" = "0" ] #OK
then
bash user_man
exit 1
elif [ "$?" = "1" ] #CANCEL
then
bash user_man
exit 1
fi
